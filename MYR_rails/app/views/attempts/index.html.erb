<p id="notice"><%= notice %></p>

<h1>Listing Attempts</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Start</th>
      <th>End</th>
      <th>Robot</th>
      <th>Mission</th>
      <th>Tracker</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @attempts.each do |attempt| %>
      <tr>
        <td><%= attempt.name %></td>
        <td><%= attempt.start %></td>
        <td><%= attempt.end %></td>
        <td><%= attempt.robot_id %></td>
        <td><%= attempt.mission_id %></td>
        <td><%= attempt.tracker_id %></td>
        <td><%= link_to 'Show', attempt %></td>
        <td><%= link_to 'Edit', edit_attempt_path(attempt) %></td>
        <td><%= link_to 'Destroy', attempt, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Attempt', new_attempt_path %>
